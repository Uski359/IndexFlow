{
    "name": "@indexflow/index-node",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "scripts": {
        "clean": "rimraf dist",
        "dev": "tsx watch src/main.ts",
        "start": "node dist/main.js",
        "build": "tsc -p tsconfig.json",
        "lint": "eslint \"src/**/*.ts\"",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "test": "vitest run",
        "test:watch": "vitest",
        "prisma:migrate": "prisma migrate dev",
        "prisma:generate": "prisma generate",
        "prisma:deploy": "prisma migrate deploy",
        "db:seed": "tsx prisma/seed.ts"
    },
    "dependencies": {
        "@indexflow/config": "file:../packages/config",
        "@apollo/server": "^4.10.0",
        "@graphql-tools/schema": "^10.0.12",
        "@prisma/client": "^6.18.0",
        "body-parser": "^1.20.3",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "ethers": "^6.10.0",
        "express": "^4.19.2",
        "graphql": "^16.9.0",
        "helmet": "^7.0.0",
        "pino": "^9.4.0",
        "pino-pretty": "^11.0.0",
        "promise-retry": "^2.0.1",
        "rate-limiter-flexible": "^2.4.2",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.5",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/helmet": "^4.0.0",
        "@types/node": "^20.11.30",
        "@types/promise-retry": "^1.1.4",
        "@types/ws": "^8.5.8",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-unused-imports": "^3.1.0",
        "prisma": "^6.18.0",
        "rimraf": "^5.0.5",
        "tsx": "^4.7.0",
        "typescript": "^5.4.5",
        "vitest": "^4.0.6"
    }
}
